#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    vector<vector<int> > permute(vector<int>& nums) {
        sort(nums.begin(), nums.end());
        int n = nums.size();
        vector<vector<int> > ret;
        do{
                ret.push_back(nums);
        }while(next_permutation(nums.begin(), nums.end()));

        return ret;
    }
};

int main()
{
    return 0;
}
